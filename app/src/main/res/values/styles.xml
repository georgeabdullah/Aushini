<resources>


    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/white</item>
    </style>

    <style name="AuDefaultTheme"    parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/color_auDefaultGreen</item>
        <item name="colorAccent">@color/color_auDefaultGreenDark</item>
        <item name="colorPrimaryDark">@color/color_auDefaultGreenDark</item>
    </style>

    <style name="AuDefaultTheme_register.scr"    parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary_register.scr</item>
        <item name="colorAccent">@color/colorAccent_register.scr</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark_register.scr</item>
    </style>

</resources>
